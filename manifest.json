{
  "manifest_version": 2,
  "name": "Upalabdh",
  "description": "Utility to monitor the availability of products at E-Commerce Stores",
  "version": "1.0",
  "background": {
    "scripts": [
      "status.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon_kr.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "https://raw.githubusercontent.com/tarunmaini16/Upalabdh/master/Util/locators.json",
    "https://*.amazon.in/*",
    "https://*.amazon.de/*",
    "https://*.amazon.com/*",
    "https://*.amazon.co.uk/*",
    "https://*.amazon.co.jp/*",
    "https://*.flipkart.in/*",
    "https://*.flipkart.com/*",
    "https://*.bigbasket.com/*"
  ],
  "web_accessible_resources": [
    "icon_kr.png"
  ],
  "icons": {
    "128": "icon_kr.png"
  }
}
